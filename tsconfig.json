{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext"],
    "alwaysStrict": true,
    "declaration": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "types": ["@cloudflare/workers-types", "@types/jest"]
  },
  "exclude": ["node_modules", "dist", "test"]
}
