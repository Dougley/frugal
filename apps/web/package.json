{
  "name": "@dougley/frugal-web",
  "private": true,
  "sideEffects": false,
  "version": "0.0.0",
  "scripts": {
    "build": "remix build",
    "build:mapped": "remix build --sourcemap",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler pages dev ./public --r2 STORAGE --kv KV_SESSIONS --d1 D1 --persist --persist-to='../../.mf'",
    "dev": "cross-env NODE_ENV=development concurrently \"npm:dev:*\"",
    "start": "npm run dev",
    "format": "prettier --write ./app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dougley/frugal.git",
    "directory": "apps/web"
  },
  "dependencies": {
    "@dougley/d1-database": "workspace:^",
    "@radix-ui/react-avatar": "^1.0.2",
    "@radix-ui/react-dialog": "^1.0.3",
    "@remix-run/cloudflare": "^1.16.0",
    "@remix-run/cloudflare-pages": "^1.16.0",
    "@remix-run/node": "^1.16.0",
    "@remix-run/react": "^1.16.0",
    "@sentry/remix": "^7.51.2",
    "cross-env": "^7.0.3",
    "daisyui": "^2.51.6",
    "date-fns": "^2.30.0",
    "isbot": "^3.6.10",
    "kysely": "^0.24.2",
    "kysely-d1": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.8.0",
    "remix-auth": "^3.4.0",
    "remix-auth-oauth2": "^1.6.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230511.0",
    "@dougley/types": "workspace:^",
    "@remix-run/dev": "^1.16.0",
    "@remix-run/eslint-config": "^1.16.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "discord-api-types": "^0.37.42",
    "eslint": "^8.40.0",
    "postcss": "^8.4.23",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "wrangler": "^2.20.0"
  },
  "engines": {
    "node": ">=16.13"
  }
