{
  "extends": "@dougley/tsconfig/workers",
  "compilerOptions": {
    "declaration": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022"
    ],
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "ES2022",
  },
  "exclude": [
    "dist"
  ]
}